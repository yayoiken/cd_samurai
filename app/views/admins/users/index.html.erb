<div>
<%= link_to "商品一覧へ", admins_products_path %><br>
<%= link_to "商品の追加" , new_admins_product_path %><br>
<%= link_to "ユーザー情報一覧" , admins_users_path %><br>
</div>

<h1> ユーザー一覧画面（管理者側）</h1>

<% @users.each do |user| %>

<%= user.id %>
<%= user.last_name + user.first_name %>
<%= user.email %>

<%= link_to "詳細" , admins_user_path(user) %>
<%= link_to "削除" , admins_user_path(user), method: :delete %><br>

<% end %>

 <%= link_to('Logout', destroy_admin_session_path, :method => :delete) %>
